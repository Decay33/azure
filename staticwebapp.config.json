{
  "routes": [
    {
      "route": "/members/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/google?post_login_redirect_uri=/members",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "google": {
          "registration": {
            "clientIdSettingName": "GOOGLE_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
            }
          },
          "login": {
            "authority": "https://accounts.google.com",
            "scope": "openid profile email",
            "loginParameters": [
              "prompt=consent"
            ]
          },
          "userDetails": {
            "uri": "https://openidconnect.googleapis.com/v1/userinfo"
          }
        }
      }
    }
  }
}
