{
  "mimeTypes": { ".wasm": "application/wasm" },
  "routes": [
    { "route": "/members/*", "allowedRoles": ["authenticated"] },
    { "route": "/api/*", "allowedRoles": ["authenticated"] }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/google?post_login_redirect_uri=/members",
      "statusCode": 302
    }
  },
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "google": {
          "registration": {
            "clientIdSettingName": "GOOGLE_CLIENT_ID",
            "clientCredential": { "clientSecretSettingName": "GOOGLE_CLIENT_SECRET" },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://accounts.google.com/.well-known/openid-configuration"
            }
          },
          "login": { "scopes": ["openid", "email", "profile"] }
        }
      }
    }
  },
  "navigationFallback": { "rewrite": "/index.html" }
}

